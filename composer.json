{
    "name": "kanagama/laravel-zettai-reach-sms-client",
    "version": "1.0.0",
    "description": "絶対リーチSMS API for Laravel",
    "keywords": [
        "laravel",
        "zettai-reach",
        "sms"
    ],
    "authors": [
        {
            "name": "kazuma nagama",
            "email": "k.nagama0632@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^11.0 | ^12.0",
        "php": "^8.2 | ^8.3 | ^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Kanagama\\ZettaiReachSmsClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
           "Kanagama\\ZettaiReachSmsClient\\Tests\\": "tests/"
        }
    },
    "exclude": [
        "tests/",
        ".docker/",
        ".github/",
        "*.md"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Kanagama\\ZettaiReachSmsClient\\V1\\ZettaiReachSmsClientServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
